{
    "$schema": "https://json.schemastore.org/prettierrc",
    "semi": true,
    "tabWidth": 4,
    "singleQuote": false,
    "printWidth": 79,
    "trailingComma": "none",
    "endOfLine": "lf",
    "overrides": [
        {
            "files": ["*.yml", "*.yaml", "*.md"],
            "options": {
                "tabWidth": 2
            }
        }
    ]
}
